programa {
  funcao inicio() {
    // Declaração de Variáveis
    inteiro numero1, numero2 , soma

    // Entrada de Dados

    escreva("Qual o primeiro número? ")
    leia(numero1)

    escreva("Qual o segundo número? ")
    leia(numero2)

    // Processamneto , Cálculo

    soma = numero1 + numero2 

    // Saída de Dados
    escreva("A soma dos dois números é igual a: " , soma)


  }
}
